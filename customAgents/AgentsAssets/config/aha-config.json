{
  "aha": {
    "domain": "YOUR_SUBDOMAIN.aha.io",
    "apiUrl": "https://YOUR_SUBDOMAIN.aha.io/api/v1",
    "authentication": {
      "apiToken": "YOUR_AHA_API_TOKEN",
      "tokenType": "Bearer"
    },
    "product": {
      "productKey": "YOUR_PRODUCT_KEY",
      "productName": "AlphaEchoCharlieOscar"
    }
  },
  "workItemMapping": {
    "epic": {
      "ahaType": "epic",
      "endpoint": "/epics",
      "defaultWorkflow": "New"
    },
    "userStory": {
      "ahaType": "feature",
      "endpoint": "/features",
      "defaultWorkflow": "New"
    },
    "requirement": {
      "ahaType": "requirement",
      "endpoint": "/requirements",
      "defaultWorkflow": "New"
    }
  },
  "customFields": {
    "storyPoints": {
      "fieldKey": "story_points",
      "fieldType": "number"
    },
    "teamAssignment": {
      "fieldKey": "team",
      "fieldType": "select"
    },
    "technicalComplexity": {
      "fieldKey": "technical_complexity",
      "fieldType": "select",
      "options": ["Low", "Medium", "High", "Very High"]
    },
    "agentCreated": {
      "fieldKey": "agent_created",
      "fieldType": "boolean"
    }
  },
  "teams": {
    "frontend": {
      "name": "Frontend Team",
      "ahaTag": "frontend-team"
    },
    "backend": {
      "name": "Backend Team",
      "ahaTag": "backend-team"
    },
    "infrastructure": {
      "name": "Infrastructure Team",
      "ahaTag": "infrastructure-team"
    },
    "business": {
      "name": "Business Team",
      "ahaTag": "business-team"
    }
  },
  "workflows": {
    "epic": ["New", "In Planning", "Ready", "In Progress", "Done"],
    "feature": ["New", "Under Review", "Ready for Dev", "In Development", "In Testing", "Shipped"],
    "requirement": ["New", "Specified", "Accepted", "Implemented"]
  },
  "priorities": {
    "p0": {
      "ahaValue": "Critical",
      "rank": 1
    },
    "p1": {
      "ahaValue": "High",
      "rank": 2
    },
    "p2": {
      "ahaValue": "Medium",
      "rank": 3
    },
    "p3": {
      "ahaValue": "Low",
      "rank": 4
    }
  },
  "releases": {
    "namingPattern": "{YYYY}-Q{Q}",
    "endpoint": "/releases"
  },
  "tags": {
    "domains": [
      "payments",
      "analytics",
      "user-management",
      "event-management",
      "ticketing",
      "reporting",
      "stripe-integration"
    ],
    "techStack": [
      "react",
      "typescript",
      "nodejs",
      "azure",
      "bicep",
      "playwright",
      "cosmos-db",
      "functions"
    ],
    "agents": [
      "product-manager",
      "requirements-agent",
      "scrum-master",
      "azure-architecture",
      "devops-agent",
      "testing-quality",
      "documentation-agent"
    ]
  },
  "apiEndpoints": {
    "epics": "{apiUrl}/epics",
    "features": "{apiUrl}/features",
    "requirements": "{apiUrl}/requirements",
    "releases": "{apiUrl}/releases",
    "ideas": "{apiUrl}/ideas",
    "goals": "{apiUrl}/goals",
    "initiatives": "{apiUrl}/initiatives"
  },
  "integration": {
    "enabled": true,
    "syncWithAzureDevOps": true,
    "webhookUrl": "https://YOUR_WEBHOOK_URL/aha-notifications",
    "autoCreateOnApproval": true
  }
}
